{
    "manifest_version": 2,
    "name":"Name Tracer",
    "version":"2.0.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":
            ["jquery.js",
              "content.js"  ]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_icon":"link-closed-flat.png"
    }

}
